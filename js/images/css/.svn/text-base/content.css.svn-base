body
{
    margin: 0px;
    color: #000;
    font-size: 13px;
}
.header
{
    height: 35px;
    line-height: 35px;
    border-bottom: 1px solid #CCCCCC;
    margin-bottom: 10px;
}
.nav_txt
{
    font-family: Arial, Helvetica, sans-serif;
    font-size: 13px;
    line-height: 25px;
    color: #666666;
}
.input
{
    width: 175px;
    height: 18px;
    line-height: 18px;
    border: 1px solid #8A9496;
}
.button
{
    width: 100px;
    height: 26px;
    line-height: 21px;
}
.bottom
{
    margin-top: 8px;
}
.tab_win
{
    width:100%; 
    height:100%;
}
.tab_win tr
{
    height:30px;
    line-height:30px;
}



